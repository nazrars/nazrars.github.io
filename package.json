  "devDependencies": {
    "@fortawesome/fontawesome-pro": "^5.11.2",
    "cpy-cli": "^2.0.0",
    "gh-pages": "^2.1.1",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.0",
    "sass": "^1.23.0",
    "typescript": "^3.6.4"
  }
