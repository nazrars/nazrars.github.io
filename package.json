    "@graphql-codegen/introspection": "1.17.6",
    "@graphql-codegen/typescript": "1.17.6",
    "@graphql-codegen/typescript-graphql-files-modules": "1.17.6",
    "@graphql-codegen/typescript-operations": "1.17.6",
    "@graphql-codegen/typescript-type-graphql": "1.17.6",
    "@vue/babel-preset-app": "^4.4.6",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-service": "4.4.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "core-js": "^3.6.5",
    "dotenv": "8.2.0",
    "eslint": "7.5.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "fibers": "5.0.0",
    "graphql": "15.3.0",
    "prettier": "2.0.5",
    "prettier-eslint": "11.0.0",
    "sass": "1.26.10",
    "typescript": "3.9.7",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.3"
  },
  "bugs": {
    "url": "https://github.com/nazrars/nazrars.github.io/issues"
  },
